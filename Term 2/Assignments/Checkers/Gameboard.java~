class Gameboard{
  public char[][] board;
  
  public Gameboard(boolean setup){
    if(setup == false){
      board = new char[][]{{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'},{'-','-','-','-','-','-','-','-'}};
    }else{
      board = new char[][]{{'-', 'r', '-', 'r', '-', 'r', '-', 'r'},{'r', '-', 'r', '-', 'r', '-', 'r', '-'}, {'-', 'r', '-', 'r', '-', 'r', '-', 'r'}, {'-', '-', '-', '-', '-', '-', '-', '-'}, {'-', '-', '-', '-', '-', '-', '-', '-'}, {'w', '-', 'w', '-', 'w', '-', 'w', '-'}, {'-', 'w', '-', 'w', '-', 'w', '-', 'w'}, {'w', '-', 'w', '-', 'w', '-', 'w', '-'}};
    }
  }
}
  
  